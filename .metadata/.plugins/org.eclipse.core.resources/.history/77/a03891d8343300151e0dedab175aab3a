package mwr.college;

public class switchExample {

	public static void main(String[] args) {
		//switch Example 
		int day = 1;
		
		switch ( day ){
		case 1: System.out.println("Mondayin");
		break; 
		case 2: System.out.println ("Tuesdayln"); 
		break;
		case 3: System.out.println ("Wednesdayin"); 
		break;
		case 4: System.out.println ("Thursdayin");
		break; 
		case 5: System.out.println ("FridayXn"); 
		break;
		case 0: 
		case 6: System.out.println ("Weekendi,n"); 
		break;
		default: System.out.println ("Error — invalid dayAn");
		break; 

	}
	}
}
