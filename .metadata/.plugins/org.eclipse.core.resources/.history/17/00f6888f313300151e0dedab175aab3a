package mawr.college;

public class LoopExamples {

	public static void main(String[] args) {
		// Example break in a for Loop
		int i;
		for (i = 1; i < 10; i = i + 1) {
			if (i == 5) {
				break;
			}
			System.out.print(i);
		}
		
		// Example continue in a for Loop
		//int i;
		for (i = 1; i < 10; i = i + 1) {
			if (i == 5) {
				continue;
			}
			System.out.println(i);
		}
	}
}
