package Mawr.College;

public class Expresisions {

	public static void main(String[] args) {
		int a = 1;
		int b = 2;
		int c = 3;
		int d = 4;
		
		int result = a + b - c + d;
		int result2 = a * b / c;
		int result3 = 1 + a * b % c;
		
		
		System.out.println(result);
		System.out.println(result2);
		System.out.println(result3);

	}

}
