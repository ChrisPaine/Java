package org.totalbeginner.tutorial;

public class Person {
	//fields
	private String name;  // name of the person
	private int maximunBooks; // most books can checkout
	
	//constructor
	public Person(){
		name = "unkown name";
		maximunBooks = 3;
	}
	public String getName() {
		return name;
	}
	
	public void setName(String anyName) {
		name = anyName;
	}

}
